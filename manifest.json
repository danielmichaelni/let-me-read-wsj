{
  "manifest_version": 2,

  "name": "Let Me Read WSJ",
  "version": "2.0.0",

  "icons": {
    "128": "icon.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.wsj.com/articles/*",
    "https://blogs.wsj.com/*"
  ]
}
